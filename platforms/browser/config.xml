<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sixten.superCPR" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Super CPR</name>
    <description>
        A Super CPR metronome tempo assistant.
    </description>
    <author email="support@610ind.com" href="http://610ind.com">
        610 Industries, LLC
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="http://610ind.com" />
    <access origin="https://610ind.com" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="orientation" value="portrait" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="lightcontent" />
</widget>
